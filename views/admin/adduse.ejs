<%- include('head') %>
<style>
    .table-box{
        width: 300px;
        float: left;
        height: 600px;
        margin: 50px 150px ;
    }
    .table-box>h3{
        height: 30px;
        line-height: 30px;
        color:#000;
        font-weight: 900;
    }
    .table-box p{
        width: 300px;
        height: 50px;
        line-height: 50px;
        text-align: right;
    }
    .table-box span{
        color: #000000;
    }
    s{
        color: red;
    }
    input[type="text"],input[type="password"]{
        width: 200px;
        height: 30px;
        border: 1px solid #D7D7D7;
        font-size: 15px;
        margin-left: 10px;
    }
    input[type="submit"]{
        width: 100px;
        height: 30px;
        line-height: 30px;
        background: red;
        color: #FFFFFF;
        font-size: 15px;
        font-weight: 900;
        border-radius: 20px;
        margin-right: 100px;
    }
</style>
<div class="table-box">
    <form action="/admin/adduseCheck">
        <p><span>用户名：<s>*</s></span><input type="text" name="use_name"></p>
        <p><span>密码：<s>*</s></span><input type="password" name="use_password"></p>
        <p><span>确认密码：<s>*</s></span><input type="password" name="use_repassword"></p>
        <p><input type="submit" value="注册"></p>
    </form>
</div>